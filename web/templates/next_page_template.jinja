<ul class="pagination">
    {% for tag in data %}
    <li
        class="page-item page_{{'ellipsis' if tag=='...' else tag}} {{ 'disabled' if loop.index==1 else '' }} {{ 'active' if loop.index==2 else '' }} {{ 'disabled' if tag=='...' else '' }} index{{loop.index}}">
        <a class="page-link">{{'...' if tag=='ellipsis' else tag}}</a>
    </li>
    {% endfor %}
</ul>